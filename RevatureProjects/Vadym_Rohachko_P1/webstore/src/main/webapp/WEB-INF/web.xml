	<servlet>
		<servlet-name>UserStore</servlet-name>
		<servlet-class>com.revature.webstore.controllers.UserStoreController</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>UserStore</servlet-name>
		<url-pattern></url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>IndexJsp</servlet-name>
		<servlet-class>com.revature.webstore.controllers.IndexJsp</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>IndexJsp</servlet-name>
		<url-pattern>/welcome</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>WebStore</servlet-name>
		<servlet-class>com.revature.webstore.controllers.StoreController</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>WebStore</servlet-name>
		<url-pattern>/log</url-pattern>
		<url-pattern>/reg</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>Register</servlet-name>
		<servlet-class>com.revature.webstore.controllers.RegisterController</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Register</servlet-name>
		<url-pattern>/register</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>Login</servlet-name>
		<servlet-class>com.revature.webstore.controllers.LoginController</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Login</servlet-name>
		<url-pattern>/login</url-pattern>
	</servlet-mapping>


	<servlet>
		<servlet-name>Logout</servlet-name>
		<servlet-class>com.revature.webstore.controllers.Logout</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Logout</servlet-name>
		<url-pattern>/logout</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>Cart</servlet-name>
		<servlet-class>com.revature.webstore.controllers.CartController</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Cart</servlet-name>
		<url-pattern>/cart</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>Pay</servlet-name>
		<servlet-class>com.revature.webstore.controllers.PaymentController</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Pay</servlet-name>
		<url-pattern>/payment</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>Invent</servlet-name>
		<servlet-class>com.revature.webstore.controllers.InventoryController</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Invent</servlet-name>
		<url-pattern>/regist</url-pattern>
		<url-pattern>/unregist</url-pattern>
	</servlet-mapping>

	<error-page>
		<error-code>404</error-code>
		<location>/error.html</location>
	</error-page>


	<session-config>
		<session-timeout>-1</session-timeout>
	</session-config>

